<configuration>
  <system.webServer>
    <handlers>
      <add name="python" path="*" verb="*" modules="FastCgiModule" scriptProcessor="D:\home\python364x64\python.exe|D:\home\python364x64\wfastcgi.py" resourceType="Unspecified" requireAccess="Script" />
    </handlers>
    <rewrite>
      <rules>
        <rule name="Static Files" stopProcessing="true">
          <match url="^.*\.(js|css|html|png|jpg|jpeg|gif|ico|eot|svg|ttf|woff|woff2)$" />
          <action type="None" />
        </rule>
        <rule name="API">
          <match url="(.*)" />
          <action type="Rewrite" url="app/main.py" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>
